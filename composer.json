{
    "name": "jmsr/ddd-laravel",
    "description": "CRUD assistant to create DDD classes and dependency injections",
    "license": "mit",
    "version": "1.1.0",
    "autoload" : {
        "psr-4":{
            "DddLaravel\\": "src/",
            "API\\": "app/src/"
        }
    },
    "authors": [
        {
            "name": "Jose Miguel Sanchez Ruiz",
            "email": "jose@kliche.es"
        }
    ],
    "minimum-stability": "dev",
